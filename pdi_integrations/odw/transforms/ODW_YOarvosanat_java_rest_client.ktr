        <class_source><![CDATA[import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.net.HttpURLConnection;
import java.net.MalformedURLException;
import java.net.URL;
import java.util.Base64;
import java.io.UnsupportedEncodingException;

// See Parameters tab
String host;
String path;
String apikey;
String apiuser;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	if (first){
		// PDI parameters
		host = getParameter("HOST"); 
		path = getParameter("URL");
		apikey = getParameter("AUTH_API_KEY");
		apiuser = getParameter("AUTH_API_USER");
	  first = false;
	}

	Object[] r = getRow();

	if (r == null) {
	  setOutputDone();
	  return false;
	}
		
 		// Local variables
		String user = apiuser + ":" + apikey; // *username*:*password* for Base64
		StringBuilder jsonfile = new StringBuilder(); // json object
		int counter = 0; // row counter

		try {
			user = "Basic " + Base64.getEncoder().encodeToString(user.getBytes("utf-8"));
		} catch (UnsupportedEncodingException e) {
			logError("Base64 Error: " + e.getMessage());
		}

		try {
			// Connection 
			URL url = new URL("https://"+host+path); //https;//server.csc.fi/apipath
			HttpURLConnection conn = (HttpURLConnection) url.openConnection();
			// Rest settings
			conn.setRequestProperty("Authorization", user);
			conn.setRequestProperty("Content-Type", "application/json");
			conn.setRequestProperty("Accept", "application/json");
			conn.setRequestMethod("GET");
			logBasic("Connecting..."); // logBasic is pdi console log message priority basic
			if (conn.getResponseCode() != 200) {
				throw new RuntimeException("Failed : HTTP error code : " + conn.getResponseCode());
			}

			// Result handling
			logBasic("Reading results...");
			BufferedReader br = new BufferedReader(new InputStreamReader(conn.getInputStream()));
			Character ch; // used for string comparing
			int c = 0; // each character is read in int format
			while ((c = br.read()) != -1) { // Read result one character at time.
				ch = (char)c; // convert int to char
				jsonfile.append((char)c); // concatenate each char to string object
			
				if(ch.toString().equals("}")){ // separate each json object
					// Create PDI outputrow
					counter++;
					if(counter % 100000 == 0) { // show every 100000 row in log
						logBasic("--- " + counter + "---");
					}
					Object[] out_row = RowDataUtil.createResizedCopy(r, data.outputRowMeta.size());  // create new output row
					get(Fields.Out, "rows").setValue(out_row, jsonfile.toString()); // insert json to output row "rows"
					putRow(data.outputRowMeta, out_row); // save output row
					jsonfile.delete(0, jsonfile.length()); //empty file
				}
			}
			logBasic("Done, total rows: " + counter);
			br.close();
			conn.disconnect();
		} catch (MalformedURLException e) {
			e.printStackTrace();
			return false;
		} catch (IOException e) {
			e.printStackTrace();
			return false;
		}
		return true; // Successful exit
}
]]></class_source>
